        {
            "_id": "_design/script",
            "language": "javascript",
            "views": {
                "javascript": {
                    "map": "function(doc) { if (doc.type === 'script' && doc.common.engineType.match(/^[jJ]ava[sS]cript|^[cC]offee[sS]cript|^[tT]ype[sS]cript|^Blockly/)) emit(doc.common.name, doc); }"
                }
            }
        }
    ]
